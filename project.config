[access]
	inheritFrom = All-Projects
[project]
	description = Gateway Agent
[access "refs/for/*"]
	pushMerge = group ldap/gateway_agent
[access "refs/heads/*"]
	abandon = group ldap/gateway_agent
	create = group ldap/gateway_agent
	forgeAuthor = group ldap/gateway_agent
	forgeCommitter = group Gateway
	push = group Gateway
	label-Code-Review = -2..+2 group Gateway
	submit = group Gateway
	editTopicName = group Gateway
[access "refs/meta/config"]
	read = group Gateway
[access "refs/tags/*"]
	pushTag = group ldap/gateway_agent
	pushSignedTag = group ldap/gateway_agent
