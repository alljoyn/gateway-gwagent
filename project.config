[access]
	inheritFrom = All-Projects
[project]
	description = Gateway Agent
	state = read only
[access "refs/for/*"]
	pushMerge = group GatewayAgent
[access "refs/heads/*"]
	abandon = group GatewayAgent
	create = group GatewayAgent
	forgeAuthor = group GatewayAgent
	forgeCommitter = group GatewayAgent
	push = group GatewayAgent
	label-Code-Review = -2..+2 group GatewayAgent
	submit = group GatewayAgent
	editTopicName = group GatewayAgent
[access "refs/meta/config"]
	read = group GatewayAgent
[access "refs/tags/*"]
	pushTag = group GatewayAgent
	pushSignedTag = group GatewayAgent
[access "refs/*"]
	read = group GitHub Replication
